{
  "name": "text-fields",
  "version": "3.1.0",
  "description": "The TextFields class is designed to manage and design text fields (input and textarea) according to Material Design principles. The main function of the class is to add visual and functional enhancements to standard HTML form elements.",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/text-fields.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/text-fields/issues"
  },
  "homepage": "https://github.com/ux-ui-pro/text-fields",
  "sideEffects": false,
  "css": "dist/text-fields.css",
  "main": "dist/text-fields.js",
  "module": "dist/text-fields.module.js",
  "targets": {
    "css": {
      "source": "src/styles/index.scss"
    },
    "main": {
      "source": "src/TextFields.js"
    },
    "module": {
      "source": "src/TextFields.js"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "clean": "rm -rf dist .parcel-cache",
    "build": "yarn clean && parcel build",
    "lint:js": "eslint --ext .js",
    "lint:scss": "stylelint **/*.scss",
    "lintfix": "yarn lint:js --fix && yarn lint:scss --fix"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.12.0",
    "eslint": "^7.32.0 || ^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "parcel": "^2.12.0",
    "stylelint": "^16.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-selector-bem-pattern": "^4.0.0"
  },
  "keywords": [
    "text",
    "field",
    "input",
    "textarea",
    "auto",
    "resizeable",
    "icons",
    "required",
    "material",
    "design",
    "mobile",
    "desktop"
  ]
}
