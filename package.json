{
  "name": "text-fields",
  "version": "2.5.1",
  "description": "Input, textarea, material",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/text-fields.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/text-fields/issues"
  },
  "css": "dist/index.css",
  "main": "dist/index.js",
  "source": "dist/index.html",
  "module": "dist/index.module.js",
  "targets": {
    "css": {
      "source": "src/index.scss"
    },
    "main": {
      "source": "src/TextFields.js"
    },
    "module": {
      "source": "src/TextFields.js"
    }
  },
  "scripts": {
    "serve": "parcel src/index.html --dist-dir dist --cache-dir .parcel --no-source-maps --open",
    "build": "parcel build",
    "clean": "rm -rf dist .parcel-cache",
    "lint:js": "eslint --ext .js .",
    "lint:scss": "stylelint **/*.{scss}",
    "lintfix": "yarn lint:js --fix && yarn lint:scss --fix"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "~2.12.0",
    "eslint": "^7.32.0 || ^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "parcel": "^2.12.0",
    "sass": "^1.72.0",
    "postcss-html": "^1.6.0",
    "stylelint": "^16.2.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-selector-bem-pattern": "^4.0.0"
  },
  "keywords": [
    "text",
    "field",
    "input",
    "textarea",
    "auto",
    "resizeable",
    "icons",
    "required",
    "material",
    "design",
    "mobile",
    "desktop"
  ]
}
